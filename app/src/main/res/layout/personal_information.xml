<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include layout="@layout/tollbar"
        android:visibility="gone"></include>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/hundred_twenty"
                android:layout_height="@dimen/hundred_twenty"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/thirty"
                android:src="@drawable/account_button">
            </ImageView>

            <com.google.android.material.textfield.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Fullname"
                app:hintTextColor="#94979A"
                app:boxStrokeColor="#94979A"
                android:layout_margin="@dimen/fifteen">

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/fifty"
                    android:padding="@dimen/ten"
                    android:textSize="@dimen/edittextsize"
                    android:textColor="#4C92D8"
                    android:inputType="text"/>

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="ContactNo"
                app:hintTextColor="#94979A"
                app:boxStrokeColor="#94979A"
                android:layout_margin="@dimen/fifteen">

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/fifty"
                    android:padding="@dimen/ten"
                    android:textSize="@dimen/edittextsize"
                    android:textColor="#4C92D8"
                    android:inputType="phone"/>

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="EmailId"
                app:hintTextColor="#94979A"
                app:boxStrokeColor="#94979A"
                android:layout_margin="@dimen/fifteen">

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/fifty"
                    android:padding="@dimen/ten"
                    android:textSize="@dimen/edittextsize"
                    android:textColor="#4C92D8"
                    android:inputType="textEmailAddress"/>

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Gender"
                app:hintTextColor="#94979A"
                app:boxStrokeColor="#94979A"
                android:layout_margin="@dimen/fifteen">

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/fifty"
                    android:padding="@dimen/ten"
                    android:textSize="@dimen/edittextsize"
                    android:textColor="#4C92D8"
                    android:inputType="text"/>

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Date Of Brith"
                app:hintTextColor="#94979A"
                app:boxStrokeColor="#94979A"
                app:endIconMode="password_toggle"
                app:endIconTint="@color/black"
                android:layout_margin="@dimen/fifteen">

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/fifty"
                    android:padding="@dimen/ten"
                    android:textSize="@dimen/edittextsize"
                    android:inputType="textPassword"
                    android:textColor="#4C92D8"/>

            </com.google.android.material.textfield.TextInputLayout>

        </LinearLayout>


    </RelativeLayout>

</RelativeLayout>