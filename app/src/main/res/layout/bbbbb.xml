            android:textColor="@color/black">

        </TextView>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@+id/text1"
            android:layout_marginTop="@dimen/ten"
            android:background="#AEA8A8" />

        <TextView
